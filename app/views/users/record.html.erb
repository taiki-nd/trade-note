<% breadcrumb :user_record %>

<div class="content">
  <h1><%= @user.nickname %>さんのトレードノート</h1>
  <div class="grid">
    <% @records.each do |record| %>
      <%= render partial: "shared/index_record", locals: { record: record } %>
    <% end %>
  </div>
  <%= paginate @records %>
</div>
<div class="space"></div>